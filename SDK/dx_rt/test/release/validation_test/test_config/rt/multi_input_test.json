{
    "model": ["dlot"],

    "configuration": {
      "dynamic-cpu-offloading": ["on"]
    },

    "threadStyle": [
      {
        "type": "single-ie",
        "count": 1
      },
      {
        "type": "multi-ie",
        "count": 3
      }
    ],

    "ieOption": {
      "ort": [true],
      "bound": ["NPU_0/2"],
      "device": ["all"]
    },

    "inferenceFunction": ["async","batch","sync"],

    "inoutOption": {
      "inputStyle": ["multi-vec", "multi-map", "multi-autosplit"],
      "outputBuffer": ["user","internal"],
      "asyncMethod": ["callback","wait"],
      "callbackDelay": 100,
      "loop": 0,
      "time": 1,
      "bitmatch": true
    }
}
